{
  "name": "express-typescript-starter",
  "version": "1.0.0",
  "description": "Project is a faster way to building a Node.js RESTful API in TypeScript.",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "^5.7.4",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "class-validator-jsonschema": "^5.0.0",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "cron-decorators": "^0.1.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "event-dispatch": "^0.4.1",
    "express": "^4.19.2",
    "express-rate-limit": "^7.2.0",
    "glob": "^10.3.12",
    "helmet": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis-cache": "^2.0.1",
    "jsonwebtoken": "^9.0.2",
    "mailgen": "^2.0.28",
    "module-alias": "^2.2.3",
    "multer": "1.4.5-lts.1",
    "node-fetch": "3.2.10",
    "nodemailer": "^6.9.13",
    "pg": "^8.11.5",
    "pino": "^9.0.0",
    "pino-http": "^9.0.0",
    "routing-controllers": "^0.10.4",
    "routing-controllers-openapi": "^4.0.0",
    "semantic-release": "^23.0.8",
    "socket-controllers": "^0.3.1",
    "swagger-ui-express": "^5.0.0",
    "typedi": "^0.10.0",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.5.1",
    "typeorm-transactional": "^0.5.0",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@faker-js/faker": "^8.4.1",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^10.0.3",
    "@types/bcrypt": "^5.0.2",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/module-alias": "^2.0.4",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.12.7",
    "@types/nodemailer": "^6.4.14",
    "@types/supertest": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/yargs": "^17.0.32",
    "@vitest/coverage-v8": "^1.5.2",
    "globals": "^15.1.0",
    "husky": "^9.0.11",
    "nodemon": "^3.1.0",
    "pino-pretty": "^11.0.0",
    "prettier": "^3.2.5",
    "reflect-metadata": "^0.2.2",
    "rifraf": "^2.0.3",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.4.5",
    "vitest": "^1.5.2"
  },
  "config": {
    "name": "default"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist coverage",
    "local": "cross-env NODE_ENV=local ./node_modules/.bin/nodemon --exec './node_modules/.bin/ts-node' src/main.ts",
    "dev": "cross-env NODE_ENV=development ./node_modules/.bin/nodemon --exec './node_modules/.bin/ts-node' src/main.ts",
    "start": "npm run build && cross-env NODE_ENV=production node ./dist/main.js",
    "typeorm": "ts-node -r ./fix-module-alias.js ./node_modules/typeorm/cli.js -d src/database/data-source",
    "typeorm-extension": "ts-node -r ./fix-module-alias.js ./node_modules/typeorm-extension/bin/cli.mjs -d src/database/data-source",
    "entity:create": "npm run typeorm -- entity:create",
    "seed:create": "typeorm-extension seed:create -n",
    "subscriber:create": "npm run typeorm subscriber:create",
    "migration:create": "npm run typeorm -- migration:create",
    "migration:generate": "npm run typeorm -- migration:generate -p true",
    "migration:run": "npm run typeorm -- migration:run",
    "migration:revert": "npm run typeorm -- migration:revert",
    "db:query": "npm run typeorm -- query",
    "db:schema:sync": "npm run typeorm -- schema:sync",
    "db:schema:log": "npm run typeorm -- schema:log",
    "db:schema:drop": "npm run typeorm -- schema:drop",
    "db:create": "npm run typeorm-extension -- db:create",
    "db:drop": "npm run typeorm-extension -- db:drop",
    "db:seed": "npm run typeorm-extension -- seed:run",
    "code:check": "prettier --check src/**/*.{ts,css,js,html}",
    "code:format": "prettier --write src/**/*.{ts,css,js,html}",
    "code:format:specific-file": "prettier --write",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:cov": "vitest --coverage",
    "cli": "ts-node -r module-alias/register src/utils/cli.ts",
    "prepare": "husky"
  },
  "keywords": [
    "expressjs",
    "express-typescript"
  ],
  "author": "Barış Gündüzer",
  "license": "MIT"
}
